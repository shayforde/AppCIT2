<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.dnet3.appcit" >
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="project.dnet3.appcit.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="project.dnet3.appcit.BlackboardActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name="project.dnet3.appcit.ViewCIT" />
        <activity android:name="project.dnet3.appcit.WebsiteActivity" />
        <activity android:name="project.dnet3.appcit.TopUpActivity" />
        <activity android:name="project.dnet3.appcit.HandbooksActivity" />
        <activity android:name="project.dnet3.appcit.StudentHandbookActivity" />
        <activity android:name="project.dnet3.appcit.SportsHandbookActivity" />


    </application>
</manifest>